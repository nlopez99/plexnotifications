{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-plusplus": 0,
      "operator-linebreak": 0,
      "no-underscore-dangle": 0,
      "quotes": 0,
      "prefer-promise-reject-errors": 0
    }
  }
  